<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".control.produto.ResultadoActivity">


    <TextView
        android:id="@+id/textDescricaoConsultarProduto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descrição:"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textTamanhoConsultarProduto"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/txtStatusConsultarProduto" />

    <TextView
        android:id="@+id/textTamanhoConsultarProduto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tamanho:"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/txtPrecoConsultarProduto"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtPrecoConsultarProduto" />

    <TextView
        android:id="@+id/txtPrecoConsultarProduto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Preço:"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textNomeConsultarProduto"
        android:layout_marginTop="18dp"
        app:layout_constraintTop_toBottomOf="@+id/textNomeConsultarProduto"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textNomeConsultarProduto" />

    <TextView
        android:id="@+id/textIDConsultarProduto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID:"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="34dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textNomeConsultarProduto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome:"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/textIDConsultarProduto"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textIDConsultarProduto" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="79dp"
        android:layout_marginTop="30dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logoasg"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/txtStatusConsultarProduto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="Status:"
        app:layout_constraintLeft_toLeftOf="@+id/textDescricaoConsultarProduto"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/textTamanhoConsultarProduto" />

</android.support.constraint.ConstraintLayout>
